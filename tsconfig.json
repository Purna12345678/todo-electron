{
  "compilerOptions": {
  "module": "commonjs",
  "moduleResolution": "node",
  "target": "esnext",
  "strict": true,
  "esModuleInterop": true,
  "skipLibCheck": true,
  "forceConsistentCasingInFileNames": true,
  "allowSyntheticDefaultImports": true,
  "jsx": "react-jsx"
},
  "include": [
    "src", 
    "vite-env.d.ts", 
    "posthtml.config.ts" 
  ],
  "exclude": [
    "node_modules" 
  ]
}

